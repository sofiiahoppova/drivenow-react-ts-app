{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",

    "allowJs": true,
    "strict": true,

    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,

    "isolatedModules": true,
    "noEmit": true,

    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "include": ["src"]
}
